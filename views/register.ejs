              <div class="col-12 mt-3">
                <label for="email">E-mail</label>
                <input type="email" id="email" name="email" />
              </div>
              <div class="col-12 mt-3">
                <label for="phone">Numer telefonu</label>
                <input type="number" id="phone" name="phone" />
              </div>
              <div class="col-12 mt-3 mb-3">
